  "main": "index.js",
  "scripts": {
    "serve:dev": "nodemon -L -e ts,js --exec 'ts-node' --transpile-only --project tsconfig.json -r tsconfig-paths/register src/index.ts",
    "serve:build": "TS_NODE_BASEURL=./build node -r tsconfig-paths/register build/src/index.js",
    "build:offline": "rimraf ./build && tsc -p tsconfig.json",
    "build:serverless": "rimraf ./build && tsc -p tsconfig-serverless.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "log4js": "^6.4.4",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.9",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript-json-schema": "^0.53.0"
  }
